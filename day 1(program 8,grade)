#include<stdio.h>
int main()
{
  int marks;
  printf("enter the marks:");
  scanf("%d",marks);
  if(marks>=90)
  {
    printf("s Grade");
  }
  else if((marks>80)&&(marks<89))
  {
    printf("A Grade");
  }
  else if((marks>70)&&(marks<79))
  {
    printf("B Grade");
  }
  else if((marks>60)&&(marks<69))
  {
    printf("C Grade");
  }
  else
  {
    printf("FAIL");
